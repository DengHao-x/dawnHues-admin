{
  "compilerOptions": {
    "baseUrl": "./",
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["vite.config.ts", "types/*.d.ts", "src/**/*.ts", "src/**/*.vue", "src/**/*","src/utils/**/*","src/store/**/*","src/hooks/**/*"],
  "exclude": ["dist", "node_modules"]
}
